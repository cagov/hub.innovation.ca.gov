{% extends "layouts/index.njk" %}
{% block content %}
<div id="page-container" class="page-container-ds">
  {# <div class="breadcrumb" aria-label="Breadcrumb" role="region">
    <a class="crumb" href="https://ca.gov" title="CA.gov">CA.gov</a>
    <span class="crumb separator">/</span>
    <a class="crumb" href="/" title="Innovation Hub">Innovation Hub</a>
    {% if parentid %}
      <span class="crumb separator">/</span>
      <span class="crumb">{{ fileids[parentid] | safe }}</span>
    {% endif %}
    <span class="crumb separator">/</span>
    <span class="crumb current">{{ title | safe }}</span>
  </div> #}

  <main id="body-content" class="cagov-main">
    <article id="post-{{ page.fileSlug }}" class="cagov-article with-single-column-wide">
      {# <h1 class="page-title">
        {{ title | safe }}
      </h1> #}
      <div class="cagov-content">
        <div class="homepage-top-block">
          <h1 class="page-title">Tips and tools for improving service delivery</h1>

          <div class="cagov-hero-body-content">
              <p class="text-lead">Principles, guides, and training to help California state staff work better.
              </p>
          </div>
        </div>
      </div>
    </article>
  </main>



</div> <!-- end page-container -->
{% block featuredContent %}
  <div class="featured-content-band">
    <div id="page-container" class="page-container-ds featured-page-container">
      <article id="post-{{ page.fileSlug }}" class="cagov-article with-single-column-wide">
        <div class="featured-resources-inner">
          {% include "featured-content.njk" %}
        </div>
      </article>
    </div>
  </div>
{% endblock %}

<div id="page-container" class="page-container-ds post-featured-page-container">
  <main id="body-content" class="cagov-main">
    <article id="post-{{ page.fileSlug }}" class="cagov-article with-single-column-wide no-top-margin">
      <div class="cagov-content no-top-margin">
        {{ content | safe }}
      </div>
    </article>
  </main>
</div>
{% endblock %}
